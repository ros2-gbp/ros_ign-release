From fc7b88f5877cc03c76dafb8c9eb52a2e49ed458c Mon Sep 17 00:00:00 2001
From: Jose Luis Rivero <jrivero@osrfoundation.org>
Date: Wed, 15 Mar 2023 18:46:19 +0100
Subject: [PATCH 3/3] Revert changed done in HEAD~2. Made in the wrong
 repository

Signed-off-by: Jose Luis Rivero <jrivero@osrfoundation.org>
---
 debian/changelog.em | 2 +-
 debian/control.em   | 5 ++---
 2 files changed, 3 insertions(+), 4 deletions(-)

diff --git a/debian/changelog.em b/debian/changelog.em
index 71ff6d51..35859090 100644
--- a/debian/changelog.em
+++ b/debian/changelog.em
@@ -1,4 +1,4 @@
-@[for change_version, change_date, changelog, main_name, main_email in changelogs]@(Package.replace('-gz-','-gzgarden-')) (@(change_version)@(DebianInc)@(Distribution)) @(Distribution); urgency=high
+@[for change_version, change_date, changelog, main_name, main_email in changelogs]@(Package) (@(change_version)@(DebianInc)@(Distribution)) @(Distribution); urgency=high
 
 @(changelog)
 
diff --git a/debian/control.em b/debian/control.em
index dc350dbe..6d7b65c3 100644
--- a/debian/control.em
+++ b/debian/control.em
@@ -1,4 +1,4 @@
-Source: @(Package.replace('-gz-','-gzgarden-'))
+Source: @(Package)
 Section: misc
 Priority: optional
 Maintainer: @(Maintainer)
@@ -6,10 +6,9 @@ Build-Depends: debhelper (>= @(debhelper_version).0.0), @(', '.join(BuildDepends
 Homepage: @(Homepage)
 Standards-Version: 3.9.2
 
-Package: @(Package.replace('-gz-','-gzgarden-'))
+Package: @(Package)
 Architecture: any
 Depends: ${shlibs:Depends}, ${misc:Depends}, @(', '.join(Depends))
-Conflicts: @(Package)
 @[if Conflicts]Conflicts: @(', '.join(Conflicts))@\n@[end if]@
 @[if Replaces]Replaces: @(', '.join(Replaces))@\n@[end if]@
 Description: @(Description)
